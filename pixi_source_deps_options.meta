        "YARP" :
        {
            "cmake-args": [
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
                "-GNinja",
                "-DYARP_COMPILE_ALL_FAKE_DEVICES:BOOL=ON",
                "-DYARP_COMPILE_GUIS=OFF"
            ]
        },
        "ICUB" :
        {
            "cmake-args": [
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
                "-GNinja",
